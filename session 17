    def Operations(a, b):
    print("Sum of two numbers is ", a + b)
    print("Difference of two numbers is ", a - b)
    print("Product of two numbers is ", a * b)
    if b != 0:
        print("Division of two numbers is ", a / b)
    else:
        print("Division by zero is not allowed")



Operations(10, 5)
